<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "passbrazil.com"
    versionCode = "3" 
    version     = "1.0.3" >

    <!-- versionCode is optional and Android only -->

    <name>PassBrazil</name>

    <description>
      Passbrazil Special Events app
    </description>

    <author href="http://www.vantagewebtech.com" email="rinku.vantage@gmail.com">
      Rinku Kamboj
    </author>
	
	<preference name="phonegap-version" value="cli-6.4.0" />
	<plugin name="cordova-plugin-device" spec="~1.1.6" />
	<platform name="android">
	  <resource-file src="google-services.json" target="google-services.json" />
	</platform>
	<platform name="ios">
	  <resource-file src="GoogleService-Info.plist" />
	  <icon src="res/icon/icon-60@3x.png" platform="ios" width="180" height="180" />
	</platform>
	<plugin name="phonegap-plugin-push" spec="~1.9.0">
		<variable name="SENDER_ID" value="1:315537388956:android:a1fc2536a0952d78" />
	</plugin>
	
	<!--<plugin name="phonegap-plugin-push"  spec="~2.0.0" />-->
	<gap:plugin name="cordova-plugin-x-socialsharing" source="npm" />
	<plugin name="cordova-plugin-inappbrowser" spec="~1.7.1" />	
	<plugin name="cordova-plugin-splashscreen" />
	
	<icon src="icon.png" />
	<icon gap:platform="webos" src="res/icon/ios/icon-180.png" />
	<icon src="res/icon/ios/icon-180.png" gap:platform="android" gap:density="ldpi" />
	<icon src="res/icon/ios/icon-180.png" gap:platform="android" gap:density="mdpi" />
	<icon src="res/icon/ios/icon-180.png" gap:platform="android" gap:density="hdpi" />
	<icon src="res/icon/ios/icon-180.png" gap:platform="android" gap:density="xhdpi" />
	<icon src="res/icon/ios/icon-180.png" gap:platform="android" gap:density="xxhdpi" />
	<icon src="res/icon/ios/icon-180.png" gap:platform="android" gap:density="ldpi" />
	<icon src="res/icon/ios/icon-180.png" gap:platform="android" gap:density="mdpi" />
	<icon src="res/icon/ios/icon-180.png" gap:platform="android" gap:density="hdpi" />
	<icon src="res/icon/ios/icon-180.png" gap:platform="android" gap:density="xhdpi" />
	<icon src="res/icon/ios/icon-180.png" gap:platform="android" gap:density="xxhdpi" />
	<icon src="res/icon/ios/icon-180.png" gap:platform="winphone" />
	<icon src="res/icon/ios/icon-180.png" gap:platform="winphone" gap:role="background" />
	
	<!--Iphone icons-->
	<preference name="SplashScreen" value="res/screen/ios/Default-568h@2x~iphone.png" />  
	<splash src="res/screen/ios/Default-568h@2x~iphone.png"/>  
  
  
	<icon height="57" platform="ios" src="res/icon/ios/icon.png" width="57" />  
	<icon height="114" platform="ios" src="res/icon/ios/icon@2x.png" width="114" />  
	<icon height="80" platform="ios" src="res/icon/ios/icon-40@3x.png" width="80" />  
	<icon height="50" platform="ios" src="res/icon/ios/icon-50.png" width="50" />  
	<icon height="100" platform="ios" src="res/icon/ios/icon-50@2x.png" width="100" />  
	<icon height="60" platform="ios" src="res/icon/ios/icon-60.png" width="60" />  
	<icon height="120" platform="ios" src="res/icon/ios/icon-60@2x.png" width="120" />  
	<icon height="180" platform="ios" src="res/icon/ios/icon-60@3x.png" width="180" />  
	<icon height="72" platform="ios" src="res/icon/ios/icon-72.png" width="72" />  
	<icon height="144" platform="ios" src="res/icon/ios/icon-72@2x.png" width="144" />  
	<icon height="76" platform="ios" src="res/icon/ios/icon-76.png" width="76" />  
	<icon height="152" platform="ios" src="res/icon/ios/icon-76@2x.png" width="152" />  
	<icon height="29" platform="ios" src="res/icon/ios/icon-small.png" width="29" />  
	<icon height="58" platform="ios" src="res/icon/ios/icon-small@2x.png" width="58" />  
	<icon height="87" platform="ios" src="res/icon/ios/icon-small@3x.png" width="87" />
	
	
	
	
	<plugin name="cordova-plugin-whitelist" spec="~1.3.2" />
    <access origin="*"/>
    <!-- Added the following intents to support the removal of whitelist code from base cordova to a plugin -->
    <!-- Whitelist configuration. Refer to https://cordova.apache.org/docs/en/edge/guide_appdev_whitelist_index.md.html -->
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
	<access origin="cdvfile://*" />
   
</widget>