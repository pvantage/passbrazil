<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "passbrazil.com"
    versionCode = "1" 
    version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>Passbrazil Special Events</name>

    <description>
      Passbrazil Special Events app
    </description>

    <author href="http://www.vantagewebtech.com" email="rinku.vantage@gmail.com">
      Rinku Kamboj
    </author>
	
	
	
	
	
	<icon gap:platform="webos" src="icons/ios/icon-180.png" />
	<icon src="icons/android.jpg" gap:platform="android" gap:density="ldpi" />
	<icon src="icons/android.jpg" gap:platform="android" gap:density="mdpi" />
	<icon src="icons/android.jpg" gap:platform="android" gap:density="hdpi" />
	<icon src="icons/android.jpg" gap:platform="android" gap:density="xhdpi" />
	<icon src="icons/android.jpg" gap:platform="android" gap:density="xxhdpi" />
	<icon src="icons/android.jpg" gap:platform="android" gap:density="ldpi" />
	<icon src="icons/android.jpg" gap:platform="android" gap:density="mdpi" />
	<icon src="icons/android.jpg" gap:platform="android" gap:density="hdpi" />
	<icon src="icons/android.jpg" gap:platform="android" gap:density="xhdpi" />
	<icon src="icons/android.jpg" gap:platform="android" gap:density="xxhdpi" />
	<icon src="icons/android.jpg" gap:platform="winphone" />
	<icon src="icons/android.jpg" gap:platform="winphone" gap:role="background" />
	
	<gap:splash src="icons/ios/screen-iphone-portrait.png" gap:platform="ios" width="180" height="180" />
	<icon src="icons/ios/icon-120.png" platform="ios" width="120" height="120" />
	<icon src="icons/ios/icon-180.png" platform="ios" width="180" height="180" />
	
	<plugin name="cordova-plugin-whitelist" spec="~1.3.2" />
    <access origin="*"/>
    <!-- Added the following intents to support the removal of whitelist code from base cordova to a plugin -->
    <!-- Whitelist configuration. Refer to https://cordova.apache.org/docs/en/edge/guide_appdev_whitelist_index.md.html -->
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
	<access origin="cdvfile://*" />
